{
    "name": "philiplb/crudlex",
    "type": "library",
    "description": "An easy to use CRUD generator for Silex.",
    "keywords": ["silex", "crud", "admin generator"],
    "homepage": "https://github.com/philiplb/CRUDlex",
    "license": "MIT",
    "authors": [
        {
            "name": "Philip Lehmann-BÃ¶hm",
            "email": "philip@philiplb.de"
        }
    ],
    "require": {
        "php": ">=5.6",
        "silex/silex": "~2.0",
        "symfony/twig-bridge": "~2.8",
        "doctrine/dbal": "~2.5",
        "symfony/yaml": "~2.8",
        "symfony/translation": "~2.8",
        "symfony/intl": "~2.8",
        "philiplb/valdi": "0.9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "satooshi/php-coveralls": "1.0.1",
        "symfony/browser-kit": "~2.8",
        "symfony/css-selector": "~2.8",
        "sphpdox/sphpdox": "dev-master",
        "eloquent/phony": "^0.13.0"
    },
    "autoload": {
        "psr-0": { "CRUDlex": "src" }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.x-dev"
        }
    }
}
